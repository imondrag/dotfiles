    name = Ivan Mondragon
    email = imondrag@umich.edu
[alias]
    ld = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=relative
    fl = log -u
    cp = cherry-pick
    s = status -s
    cl = clone
    cm = commit
    co = checkout
    br = branch
    diff = diff --word-diff
    dc = diff --cached
